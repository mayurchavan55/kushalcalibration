<!DOCTYPE html>
<html lang="en">

<head>
	<%- include( '../partial/commonCss' ) %>
</head>

<body>
	<!--Preloader-->
	<div class="preloader-it">
		<div class="la-anim-1"></div>
	</div>
	<!--/Preloader-->
	<div class="wrapper  theme-1-active pimary-color-blue">

		<%- include( '../partial/header' ) %>

			<%- include( '../partial/sidebar' ) %>

				<!-- Main Content -->
				<div class="page-wrapper">
					<div class="container-fluid">

						<!-- Title -->
						<div class="row heading-bg">
							<div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
								<h5 class="txt-dark">Customer Details</h5>
							</div>
							<!-- Breadcrumb -->
							<div class="col-lg-9 col-sm-8 col-md-8 col-xs-12 text-right">
								<button class="btn  btn-primary btn-sm btn-rounded" onclick="userDetailsModel()"><i
										class="fa fa-plus"></i> Add New
									Customer</button>
							</div>
							<!-- /Breadcrumb -->
						</div>
						<!-- /Title -->

						<!-- Row -->
						<div class="row">
							<div class="col-sm-12">
								<div class="panel panel-default card-view">
									<div class="panel-heading">
										<div class="pull-left">
											<h6 class="panel-title txt-dark">Customer Details</h6>
										</div>
										<div class="clearfix"></div>
									</div>
									<div class="panel-wrapper collapse in">
										<div class="panel-body">
											<div class="table-wrap">
												<div class="table-responsive">
													<table id="customerMasterTable" class="table table-hover display  pb-30">
														<thead>
															<tr>
																<th>Sr.no</th>
																<th>Name</th>
																<th>company name</th>
																<th>Email</th>
																<th>Mobile</th>
																<th>Address</th>
																<th>Action</th>
															</tr>
														</thead>
														
														
														<tbody>
														
															
														</tbody>
													</table>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="panel-wrapper collapse in">
							<div class="panel-body">
								<!-- sample modal content -->
								<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" id="customerModal"
									aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
									<input type="hidden" id="customereditId">
									<div class="modal-dialog modal-lg">
										<div class="modal-content">
											<div class="modal-header">
												<button type="button" class="close" data-dismiss="modal"
													aria-hidden="true">Ã—</button>
												<h5 class="modal-title" id="myLargeModalLabel">Customer Management</h5>
											</div>
											<div class="modal-body">
												<div class="row">
													<div class="col-md-6">
														<div class="form-group">
															<label class="control-label mb-10 text-left">Contact Person
																Name</label>
															<input type="text" id="contact-name" class="form-control">
															<small class="text-danger" id="contact-name-error"></small>
														</div>
													</div>
													<div class="col-md-6">
														<div class="form-group">
															<label class="control-label mb-10 text-left">Company
																Name</label>
															<input type="text" id="company-name" class="form-control">
															<small class="text-danger" id="company-name-error"></small>
														</div>
													</div>
												</div>

												<div class="row">
													<div class="col-md-6">
														<div class="form-group">
															<label class="control-label mb-10 text-left">Email</label>
															<input type="text" id="email" class="form-control">
															<small class="text-danger" id="email-error"></small>
														</div>
													</div>
													<div class="col-md-6">
														<div class="form-group">
															<label class="control-label mb-10 text-left">Mobile
																Number</label>
															<input type="number" id="mobile" class="form-control" maxlength="10">
															<small class="text-danger" id="mobile-error"></small>
														</div>
													</div>
												</div>

												<div class="row">
													<div class="col-md-6">
														<div class="form-group">
															<label class="control-label mb-10 text-left">Address</label>
															<input type="text" id="address" class="form-control">
															<small class="text-danger" id="address-error"></small>
														</div>
													</div>
												</div>


											</div>
											<div class="modal-footer">
												<button type="submit" class="btn btn-success  mr-10 submit-customer" >Submit</button>

												<button type="button" class="btn btn-danger text-left"
													data-dismiss="modal" onclick="customerManagementModelClose()">Close</button>
											</div>
										</div>
										<!-- /.modal-content -->
									</div>
									<!-- /.modal-dialog -->
								</div>
							</div>
						</div>


					</div>

					<%- include( '../partial/footer' ) %>

				</div>
				<!-- /Main Content -->

	</div>
	<!-- /#wrapper -->

	<!-- JavaScript -->

	<!-- jQuery -->

	<%- include( '../partial/commonJs' ) %>
		<script src="dist/custom/js/customermengement.js"></script>
</body>

</html>